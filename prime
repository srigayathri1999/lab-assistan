/*prime or not*/
int main() {
   int n,rem,i,ctr=0;
   printf("enter the number:");
   scanf("%d",&n);
   for(i=1;i<=n;i++);
   {
       rem=n%i;
       if(rem==0)
       ctr++;
   }
   if(ctr==2)
   printf("given number is prime");
   else {
   printf("given number is not prime");
   }
    return 0;
}
